{{#each favorites.questions as |question|}}
{{#link-to 'question' question.id class="goQ"}}
  <div class="card">
    <div class="content">
      <p>
        {{question.content}}
      </p>
    </div>
    <div class="author">
      <p>
        {{on-fire question}}
      </p>
      <p class="author">
        {{question.questionAuthor}}
      </p>
    </div>
  </div>
  {{/link-to}}
{{/each}}
